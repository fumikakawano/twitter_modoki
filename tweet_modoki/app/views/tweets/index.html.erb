<%= form_for :tweet, url: tweets_path do |f| %>
  <p>
    <%= f.text_field :tweet %>
  </p>
  <p>
    <%= f.submit "投稿" %>
  </p>
<% end %>

<% @tweets.each do |tweet| %>
    <p><%= tweet.tweet %></p>
    <p><%= tweet.created_at %></p>
    <%= link_to("削除", "/tweets/#{@tweet.id}", {method:"destroy"}) %>
<% end %>
